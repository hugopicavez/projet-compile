program
    a,b,c : boolean;
    d,e,f : integer;
    t,y,u : real;
    t1, t2 : array [3 .. 6] of array [1 .. 9] of integer;
begin
   a = true;
   b = a;
   c = a AND false;
   write("true : ", a);
   new_line();
   write("true : ", b);
   new_line();
   write("false : ", c);
   new_line();

   d = 10;
   e = d;
   f = d + 20;
   write("10 : ", d);
   new_line();
   write("10 : ", e);
   new_line();
   write("30 : ", f);
   new_line();

   for d := 3 to 6 do
        for e := 1 to 9 do
            t1[d][e] = d+e;
            t2[d][e] = t1[d][e];
            write("value : ",d+e);
            write("t1 : ", t1[d][e]);
            write("t2 : ", t2[d][e]);
        end;
   end;



end.