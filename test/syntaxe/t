. . . Noeud.ListeInst                                        -- ligne : 17
. . . . Noeud.Vide                                           -- ligne : 20
. . . . Noeud.Nop                                            -- ligne : 17
program
   x : integer; 
   y : integer; 
   i : integer; 
begin
   x := 4;
   y := 9;
   for i := 8 downto 0 do 
      if caca or coco then 
         if 3 > 2 then 
            x := x / y;
            x := x / y;
         else
            null;
         end;
      else
         null;
      end;
   end;
   if x /= y then 
      if x /= y then 
         x := x mod y;
      else
         null;
      end;
   else
      null;
   end;
end.
