. . . . . . . . . Noeud.Vide                                 -- ligne : 40
. . . . . . . . . Noeud.Chaine "Valeur "                     -- ligne : 40
. . . . . . . . Noeud.Ident "val"                            -- ligne : 40
. . . . . . . Noeud.Chaine " non trouvee"                    -- ligne : 40
. . Noeud.Ligne                                              -- ligne : 42
program
   t : array [1 .. 20] of integer; 
   i : 1 .. 20; 
   min, max, milieu : 1 .. 20; 
   val : integer; 
   trouve : boolean; 
begin
   for i := 1 to 20 do 
      t[i] := 2 * i;
   end;
   write("Valeur a chercher dans le tableau :");
   read(val);
   min := 1;
   max := 20;
   trouve := false;
   while (min <= max) and not trouve do
      milieu := (min + max) div 2;
      if t[milieu] = val then 
         trouve := true;
      else
         if t[milieu] < val then 
            min := milieu + 1;
         else
            max := milieu - 1;
         end;
      end;
   end;
   if trouve then 
      write("Trouve : T[", milieu, "] = ", t[milieu]);
   else
      write("Valeur ", val, " non trouvee");
   end;
   new_line;
end.
